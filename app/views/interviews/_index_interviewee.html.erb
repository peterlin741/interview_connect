Interviewers

<ul>
  <% @interviewers.each do |user| %>
    <li><%= user.first_name %> <%= user.last_name %></li>
  <% end %>
</ul>


<br /> <br />

<h2>Enter a few available times for the upcoming weeks</h2>




<%= form_for [@interview] do |f| %>

  <%= f.text_field :interviewee_email, :value => current_user.email, :readonly => true %>

  <%= f.datetime_select(:interview_date, prompt: {day: 'Choose day', month: 'Choose month', year: 'Choose year'}, :start_year => 2016, :ampm => true, :minute_step => 15) %>

  <p><%= f.submit %></p>

<% end %>


